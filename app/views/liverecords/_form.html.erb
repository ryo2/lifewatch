<%= form_for(@liverecord) do |f| %>
  <% if @liverecord.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@liverecord.errors.count, "error") %> prohibited this liverecord from being saved:</h2>

      <ul>
      <% @liverecord.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
